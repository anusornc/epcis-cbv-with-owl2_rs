# EPCIS 2.0 Core Classes
epcis:ObjectEvent rdf:type rdfs:Class ;
    rdfs:label "Object Event" ;
    rdfs:comment "An event that records the association of one or more objects with a business step at a specific point in time and location" .

epcis:AggregationEvent rdf:type rdfs:Class ;
    rdfs:label "Aggregation Event" ;
    rdfs:comment "An event that records the physical aggregation of objects into a parent object" .

epcis:TransactionEvent rdf:type rdfs:Class ;
    rdfs:label "Transaction Event" ;
    rdfs:comment "An event that records the transfer of custody or ownership of objects" .

epcis:TransformationEvent rdf:type rdfs:Class ;
    rdfs:label "Transformation Event" ;
    rdfs:comment "An event that records the transformation of input objects into output objects" .

